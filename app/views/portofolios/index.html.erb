<h1>Portofolio</h1>

<% @portofolio_items.each do |portofolio_item|%>
    <p><%= portofolio_item.title %></p>
    <p><%= portofolio_item.subtitle %></p>
    <p><%= portofolio_item.body %></p>
    <p><%= portofolio_item.main_image %></p>
    <p><%= image_tag  portofolio_item.thumb_image unless  portofolio_item.thumb_image.nil?    %></p>




<%end%>